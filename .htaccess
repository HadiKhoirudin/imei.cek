<IfModule mod_headers.c>
    Header add Access-Control-Allow-Origin "https://hadikhoirudin.github.io"
    Header set Access-Control-Allow-Origin "https://hadikhoirudin.github.io"
    Header always set Access-Control-Allow-Origin "https://hadikhoirudin.github.io"
</IfModule>
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>
